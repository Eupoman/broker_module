<script type="text/javascript">

	$(function() {
		$('.chosen-select').chosen({width: "100%"});
		$(".select2_demo_2").select2({width: "100%"});

		$('.i-checks').iCheck({
	        checkboxClass: 'icheckbox_square-green',
	        radioClass: 'iradio_square-green',
	    });

	    $('.pregnant').on('click', function() {
	        if($(this).find('div').hasClass('checked')) {
	        	$(".forpregnant").removeClass('hide');
	        	$(".forages").addClass('hide');
	        } else {
	        	$(".forpregnant").addClass('hide');
	        	$(".forages").removeClass('hide');
	        }
	    });

	    $('#birthdate').datetimepicker({
			locale: 'es',
		});
	});

	$("#travelnext").on('click', function(e) {
		var count = $("#memberscount").val();
		var ages = $("#ages").val();
		var res = ages.split(",");
		if ($('.pregnant div').hasClass('checked')) {
			e.preventDefault();
			window.location.href = "/getquote/travel_productlist";
		} else {
			if (parseInt(count, 10) == parseInt(res.length, 10)) {
				e.preventDefault();
				window.location.href = "/getquote/travel_productlist";
				return;
			} else {
				alert("Ingrese la edad correctamente");
				e.preventDefault();
				return;
			}
		}
	});

	$(".housenext").on('click', function(e) {
		var rut = $("#houserut").val();
		var tel = $("#housetel").val();
		var email = $("#houseemail").val();
		e.preventDefault();
		window.location.href = "/getquote/house_productlist?rut=" + rut + "&tel=" + tel + "&email=" + email;
	});

	$('.telnumber').toArray().forEach(function(field){
        new Cleave(field, {
            numericOnly: true,
            blocks: [9]
        });
    });

    $('.rut').toArray().forEach(function(field){
        new Cleave(field, {
            numericOnly: true,
            delimiters: ['-'],
            blocks: [7, 1]
        });
    });

    // With JQuery
	$("#structure").slider();
	$("#structure").on("slide", function(slideEvt) {
		$("#structureLabel span").text(slideEvt.value);
	});

